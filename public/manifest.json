{
    "name": "PullRequestToggleOwnerFiles",
    "version": "0.6",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png"
    },
    "description": "Will automatically select 'Files owned by you' in case of large pull requests (more than 10 files or total 150 additions and/or removals).",
    "author": "Fritjof HÃ¶st",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "scripting",
      "tabs",
      "storage"
    ],
    "host_permissions": [
        "https://github.com/*",
        "https://www.github.com/*"
    ]
}